{
  "name": "cloudflare-r2-workers-image",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "test:build": "tsc --project tsconfig.test.json",
    "test": "npm run test:build && node --test --experimental-test-isolation=none",
    "test:coverage": "npm run test:build && node --test --experimental-test-isolation=none --experimental-test-coverage --test-coverage-include=.test-dist/src/**/*.js --test-coverage-lines=90 --test-coverage-functions=90 --test-coverage-branches=85"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "typescript": "^5.7.3",
    "wrangler": "^4.0.0"
  }
}
